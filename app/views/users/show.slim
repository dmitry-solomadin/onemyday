div#showUserDiv
  div.innerContainer
    div.infoContainer
      = image_tag @user.avatar.url :small, width: '50', class: 'avatar'
      div.userinfo
        div.username = @user.name
    div.tabsContainer
      ul#userTabs.tabs
        li.selected
          a href="#":span His
        li
          a href="#":span Liked
